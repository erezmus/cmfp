---
import schools from "@/config/schools.json";



---

<div>
  <div class="d-flex">

    <input type="text" class="w-full" />
  </div>

  <ul class="w-full list-none m-0 p-0">
    {schools.map(school => (
      <li class="p-4 border-green-700 border-l-2 bg-blue-50">
    {school.name}
  </li>
  ))}
</ul>

</div>
